print("Q2")
#a
import panda as pd
df = pd.read_csv("weather.csv")
print(df.head(5))
#b
print(df.head(10))
#c
print(df['MinTemp'].describe())
print(df['MaxTemp'].describe())
#d
print(df.tail(5))
#e
finaldata=[df.iloc[:2].sum(),
df.iloc[:,2].mean(),
df.iloc[:,3].median(),
df.iloc[:,2].nunique(),
df.iloc[:,2].max(),
df.iloc[:,2].min()]
print(finaldata)
