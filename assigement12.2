import threading
import time
"""
In this question we are importing thread and time
"""
"""
In this we are creating a def function called
which is used to cal the square of the list
"""
#n is the number
def cal_square(number):
    print("cal square number")
    for n in number:
        time.sleep(5)
        print("square",n*n)
        
def cal_cube(number):
    print("calculate number")
    for n in number:
        time.sleep(5)
        print("cube",n*n*n)
list = [2,4,6,8]
t = time.time()
t1= threading.Thread(target=cal_square, args=(list,))
t2= threading.Thread(target=cal_cube, args=(list,))
t1.start()
t2.start()
t1.join()
t2.join()
print("done in", time.time()-t)
